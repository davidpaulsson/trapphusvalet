{
  "name": "trapphusvalet",
  "version": "1.0.0",
  "main": "index.js",
  "author": "David Paulsson <david@davidpaulsson.se>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "yarn run build && yarn run export && push-dir --dir=out --branch=gh-pages --cleanup",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "next": "^6.0.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.5",
    "push-dir": "^0.4.1"
  }
}
